{
  "functions": {
    "app/api/email/route.ts": {
      "memory": 256,
      "maxDuration": 10,
      "includeFiles": "node_modules/@prisma/engines/**"
    },
    "app/api/admin/health/route.ts": {
      "memory": 128,
      "maxDuration": 15,
      "includeFiles": "node_modules/@prisma/engines/**"
    },
    "app/api/**": {
      "memory": 128,
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "PRISMA_CLI_FORCE_INSTALL": "true",
      "PRISMA_SKIP_POSTINSTALL_GENERATE": "true"
    }
  }
}