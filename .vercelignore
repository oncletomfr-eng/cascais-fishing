# ULTRA-AGGRESSIVE - exclude ALL test pages and heavy dependencies

# Safe Next.js cache (keep manifests, server chunks!)
.next/cache/
.next/trace/

# ALL TEST PAGES - HUGE SIZE REDUCTION!
app/test-*/
public/test-*

# Test files (safe to exclude)
__tests__/
*.test.ts
*.test.tsx  
*.spec.ts
*.spec.tsx

# Development files (safe)
debug-*.js
debug-*.mjs
*.log
tmp/

# MASSIVE node_modules exclusions
node_modules/@types/
node_modules/typescript/
node_modules/.cache/
node_modules/jest/
node_modules/playwright/
node_modules/@playwright/
node_modules/webpack/
node_modules/@babel/
node_modules/@mui/x-data-grid-pro/
node_modules/date-fns/
node_modules/date-fns-jalali/
node_modules/effect/
node_modules/pdf-lib/

# Documentation (safe)
*.md
docs/

# Source maps (large, not needed in production)
*.js.map
*.d.ts.map
