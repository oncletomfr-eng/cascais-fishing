# SURGICAL exclusions - big files but KEEP routing essentials

# Safe Next.js cache (keep manifests, server chunks!)
.next/cache/
.next/trace/

# Test files (safe to exclude)
__tests__/
*.test.ts
*.test.tsx  
*.spec.ts
*.spec.tsx

# Development files (safe)
debug-*.js
debug-*.mjs
*.log
tmp/

# Large node_modules subdirs (keep core!)
node_modules/@types/
node_modules/typescript/
node_modules/.cache/
node_modules/jest/
node_modules/playwright/
node_modules/@playwright/
node_modules/webpack/

# Documentation (safe)
*.md
docs/

# Source maps (large, not needed in production)
*.js.map
*.d.ts.map
